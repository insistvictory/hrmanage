<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xux.ssm.dao.ArgumentDao">
    <insert id="addArgument">
      insert into argument (salary_id,reason,arg_salary) values (#{salaryId},#{reason},#{argSalary});
    </insert>
    <select id="queryAllArgument" resultType="Argument">
        SELECT *from argument;
    </select>
</mapper>






		
